{
  "name": "nodejs-encryption-api-example",
  "version": "1.1.0",
  "description": "Example of encrypting/decrypting data thru an API using node.js",
  "main": "src/index.js",
  "private": true,
  "snyk": true,
  "scripts": {
    "start": "node src/index",
    "pretest": "eslint ./src",
    "test": "./node_modules/.bin/mocha --reporter spec",
    "test:coverage": "./node_modules/.bin/nyc npm test",
    "test:functional": "./node_modules/.bin/mocha --reporter spec ./test/functional",
    "test:all": "./node_modules/.bin/run-s test:functional test:coverage",
    "docker:status": "node src/healthcheck",
    "snyk-protect": "snyk-protect",
    "prepare": "npm run snyk-protect"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/timoa/nodejs-encryption-api-example.git"
  },
  "keywords": [
    "encryption",
    "api",
    "endpoint",
    "datastore"
  ],
  "author": {
    "name": "Damien Laureaux",
    "email": "d.laureaux@timoa.com",
    "url": "https://timoa.com"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/timoa/nodejs-encryption-api-example/issues"
  },
  "homepage": "https://github.com/timoa/nodejs-encryption-api-example#readme",
  "dependencies": {
    "@snyk/protect": "1.903.0",
    "@hapi/boom": "9.1.4",
    "fastify": "3.28.0",
    "fastify-healthcheck": "3.1.0",
    "fastify-swagger": "5.1.1",
    "lodash": "4.17.21",
    "mongoose": "5.13.14",
    "uuid": "8.3.2",
    "winston": "3.7.2"
  },
  "engines": {
    "node": ">=16.0",
    "npm": ">=8.6.0"
  },
  "os": [
    "linux",
    "win32",
    "darwin"
  ],
  "devDependencies": {
    "acorn": "8.7.0",
    "acorn-jsx": "5.3.2",
    "chai": "4.3.6",
    "chai-as-promised": "7.1.1",
    "chai-http": "4.3.0",
    "eslint": "8.13.0",
    "eslint-config-airbnb": "19.0.4",
    "eslint-plugin-import": "2.26.0",
    "eslint-plugin-jsx-a11y": "6.5.1",
    "eslint-plugin-react": "7.29.4",
    "espree": "9.3.1",
    "mocha": "9.2.2",
    "npm-run-all": "4.1.5",
    "nyc": "15.1.0",
    "request": "2.88.2"
  }
}
